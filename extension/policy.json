{
    "SHORT_COMPLEX": {
        "MIN_LENGTH": 8,
        "REGEX": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[^\\da-zA-Z]).{8,}$"
    },
    "LONG_SIMPLE": {
        "MIN_LENGTH": 20,
        "REGEX": "^(?=.*[a-z])(?=.*[A-Z])[a-zA-Z\\d]{20,}|^(?=.*[a-z])(?=.*[0-9])[a-z0-9]{20,}|^(?=.*[A-Z])(?=.*[0-9])[A-Z0-9]{20,}|(?=.*[\\w])(?=.*[\\W])[\\w\\W]{20,}"
    }
}
